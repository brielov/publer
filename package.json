{
  "name": "publer",
  "version": "1.1.1",
  "description": "A tiny, type-safe pubsub library",
  "main": "dist/publer.js",
  "module": "dist/publer.mjs",
  "typings": "dist/publer.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "pub/sub",
    "pubsub",
    "publish/subscribe",
    "publish",
    "subscribe"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "homepage": "https://github.com/brielov/publer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/publer.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/publer/issues"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "esbuild": "^0.14.25",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
